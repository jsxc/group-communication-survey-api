{
  "name": "group-communication-survey-api",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run start-watch",
    "start-watch": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "consola": "^2.11.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "moment": "^2.24.0",
    "mongoose": "^5.8.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "^0.0.45",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^13.7.0",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,css,md}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  }
}
